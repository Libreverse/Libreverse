{
  "name": "libreverse",
  "private": true,
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs rollup.config.js",
    "build:css": "gulp build",
    "devbuild": "rollup -c -w --bundleConfigAsCjs rollup.config.js",
    "devbuild:css": "gulp devbuild",
    "prepare": "husky",
    "format": "prettier . --write",
    "analyse:security": "codeql database create db --language=ruby,javascript && codeql database analyze db --format=sarif-latest --output=results.sarif -- codeql-packs/ruby-queries,codeql-packs/js-queries"
  },
