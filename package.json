    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@sentry/browser": "^8.35.0",
    "core-js": "^3.38.1",
    "cssnano": "^7.0.6",
    "cssnano-preset-advanced": "^7.0.6",
    "gulp": "^5.0.0",
    "gulp-postcss": "^10.0.0",
    "gulp-sass": "^5.1.0",
