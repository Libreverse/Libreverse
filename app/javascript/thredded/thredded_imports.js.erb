<% timeago_root =
  File.join(
    Gem.loaded_specs["timeago_js"].full_gem_path,
    "assets",
    "javascripts",
  ) %>
import "<%= File.join(timeago_root, "timeago.js") %>";
<%= %w[de pt_BR].map do |locale|
  %(import "#{File.join(timeago_root, "timeago/locales/#{locale}.js")}";)
end * "\n" %>
<%= Thredded::WebpackAssets.javascripts %>
