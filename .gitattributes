# Store all files above 100MB in Git LFS
* filter=lfs diff=lfs merge=lfs -text if size > 100MB
# Mark db/schema.rb as linguist-generated
db/schema.rb linguist-generated=true
# Mark all files in vendor/ as linguist-vendored
vendor/* linguist-vendored=true
# Apply rails_credentials diff driver to encrypted credential files
config/credentials/*.yml.enc diff=rails_credentials
config/credentials.yml.enc diff=rails_credentials
